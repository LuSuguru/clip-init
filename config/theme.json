{
  "@text-color": "fade(@black, 65%)",
  "@logo-deep": "#223151",
  "@rich-text": "#444444",
  "@description-text": "#6c757d",
  "@codemirror-placeholder": "#b9b9b9",
  "@body-background": "#eff2f7",
  "@light-bg": "#f7f7f7",

  "@secondary-color": "@logo-deep",
  "@light-text-color": "lighten(@text-color, 25%)",
  "@disabled-text-color": "lighten(@text-color, 35%)",
  "@disabled-icon-color": "lighten(@text-color, 75%)",
  "@border-color-base": "#d9d9d9",
  "@grey-line-color": "rgba(0, 0, 0, .05)",
  "@header-index": "2",

  "@popover-box-shadow": "0 1px 6px rgba(0, 0, 0, .2)",
  "@header-box-shadow": "0 2px 1px 0 rgba(0, 0, 0, .1)",
  "@left-side-box-shadow": "2px 0 1px 0 rgba(0, 0, 0, .1)",
  "@right-side-box-shadow": "-2px 0 1px 0 rgba(0, 0, 0, .1)",
  "@block-box-shadow": "0 2px 3px 0 rgba(0, 0, 0, .05)",
  "@block-active-box-shadow": "0 7px 21px 0 rgba(0, 0, 0, .15)",

  "@white": "#fff",
  "@black": "#000",
  "@red": "#F04134",
  "@green": "#52C41B",
  "@yellow": "#FFCE08",
  "@blue": "#4997f1",
  "@pick": "#DB7093",
  "@primary-color": "#ff8a65",
  "@second-color": "#b26046",
  "@light-green": "#8BC34A"
}