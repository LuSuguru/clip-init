  "name": "{{projectName}}",
  "version": "{{projectVersion}}",
  "description": "{{projectDescription}}",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config config/webpack.dev.conf.js  --progress",
    "build": "node config/build.js",
    "analyzer": "node config/build.js --analyzer true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LuSuguru/clip-init.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LuSuguru/clip-init/issues"
  },
  "homepage": "https://github.com/LuSuguru/clip-init#readme",
  "dependencies": {
    "core-js": "2",
    "object-assign": "^4.1.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.4.5",
    "autoprefixer": "^9.5.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.11.2",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-flowtype": "^3.9.1",
    "eslint-plugin-html": "^5.0.5",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.6.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "ora": "^3.0.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.32.2",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1",
    "webpack-merge": "^4.2.1"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true
    },
    "parserOptions": {
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "no-const-assign": "warn",
      "no-this-before-super": "warn",
      "no-unreachable": "warn",
      "no-unused-vars": "warn",
      "constructor-super": "warn",
      "valid-typeof": "warn",
      "no-mixed-spaces-and-tabs": [
        2,
        false
      ],
      "react/jsx-filename-extension": 0,
      "no-debugger": 0,
      "no-console": 0,
      "jsx-a11y/alt-text": 0,
      "max-len": 0,
      "react/prop-types": 0,
      "eqeqeq": 0,
      "comma-dangle": "off",
      "react/jsx-no-bind": "off",
      "react/no-array-index-key": 0,
      "func-names": 0,
      "no-param-reassign": 0,
      "class-methods-use-this": 0,
      "no-undef": 1,
      "jsx-a11y/click-events-have-key-events": 0,
      "jsx-a11y/no-static-element-interactions": 0,
      "arrow-parens": 0,
      "import/no-extraneous-dependencies": 0,
      "global-require": 0,
      "import/order": 0,
      "prefer-arrow-callback": 0,
      "no-useless-return": 0,
      "quotes": 0,
      "consistent-return": 0,
      "import/no-dynamic-require": 0,
      "import/no-unresolved": 0,
      "react/destructuring-assignment": 0,
      "react/jsx-one-expression-per-line": 0,
      "react/jsx-closing-bracket-location": 0,
      "import/prefer-default-export": 0,
      "lines-between-class-members": 0,
      "react/jsx-wrap-multilines": 0,
      "import/named": 0,
      "object-property-newline": 0,
      "object-curly-newline": 0,
      "import/no-mutable-exports": 0,
      "guard-for-in": 0,
      "no-restricted-syntax": 0,
      "camelcase": 0,
      "react/jsx-indent": 0,
      "no-nested-ternary": 0,
      "jsx-a11y/no-noninteractive-element-interactions": 0,
      "no-shadow": 0,
      "no-return-assign": 0,
      "no-unused-expressions": 0,
      "no-useless-escape": 0,
      "react/sort-comp": 0,
      "prefer-template": 0,
      "react/no-unescaped-entities": 0,
      "no-alert": 0,
      "default-case": 0,
      "no-plusplus": 0,
      "no-mixed-operators": 0,
      "no-continue": 0,
      "jsx-a11y/label-has-for": 0,
      "react/button-has-type": 0,
      "jsx-a11y/anchor-is-valid": 0,
      "import/no-cycle": 0,
      "react/no-multi-comp": 0,
      "react/no-did-update-set-state": 0,
      "semi": [
        "error",
        "never"
      ]
    }
  }
}
